    <form method="post" action="/valida" class="form-stacked">
      <div class="clearfix">

        <label for="nombre">Nombre</label>

        <div class="input">
          <input class="large" name="nombre" size="30" type="text"/>
        </div>
        <span class="help-block">Campo Obligatorio</span>

      </div>
      <!-- /clearfix -->
