            <span class="add-on">#</span>
            <input class="medium" name="telefono" size="20" type="text"/>
          </div>
          <span class="help-block">Campo Obligatorio</span>
        </div>
      </div>
      <div class="clearfix">
        <label for="stackedSelect">Estado</label>


      </div>
      <!-- /clearfix -->


      <div class="clearfix">
        <label for="textarea">Pregunta</label>

        <div class="input">
          <textarea class="xxlarge" name="pregunta" rows="3"></textarea>
                                    <span class="help-block"> Escribe tu comentario o pregunta
                                    </span>

        </div>
      </div>

      <!-- /clearfix -->
      <br/>


      &nbsp;
      <button class="btn success" type="submit">Enviar</button>
      &nbsp;
      <button class="btn danger" type="reset">Limpiar</button>

